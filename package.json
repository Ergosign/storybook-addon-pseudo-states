{
  "name": "@ergosign/storybook-pseudo-states",
  "version": "0.0.1",
  "description": "Storybook decorator to display all pseudo states at once.",
  "directories": {
    "test": "test",
    "dist": "dist"
  },
  "scripts": {
    "lint": "eslint src --fix",
    "angular:build": "tsc --p src/angular",
    "angular:pack": "cd src/angular && npm pack",
    "react:build": "tsc --p src/react",
    "react:pack": "cd src/react && npm pack",
    "html:build": "tsc --p src/html",
    "html:pack": "cd src/html && npm pack",
    "lit:build": "tsc --p src/lit",
    "lit:pack": "cd src/lit && npm pack",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:version": "lerna version",
    "lerna:build-all": "lerna run build"
  },
  "author": "philippone",
  "license": "MIT",
  "devDependencies": {
    "@angular/core": "8.2.14",
    "@babel/core": "7.7.2",
    "@storybook/addons": "5.2.6",
    "@storybook/cli": "5.2.6",
    "@storybook/html": "5.2.6",
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "babel-loader": "8.0.6",
    "cz-conventional-changelog": "3.0.2",
    "emotion": "10.0.23",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lerna": "3.18.5",
    "lit-html": "1.1.2",
    "typescript": "^3.8.0-dev.20191120",
    "zone.js": "0.10.2"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
