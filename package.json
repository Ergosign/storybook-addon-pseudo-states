{
  "name": "@ergosign/addon-pseudo-states",
  "version": "0.0.1-beta.3",
  "description": "Storybook Addon to show pseudo states",
  "directories": {
    "test": "test",
    "dist": "dist"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch --outDir dist",
    "prepack-package": "cp package.json ./dist",
    "release-it-beta": "release-it -n --preRelease=beta --npm.tag=next --verbose",
    "release-it-patch": "release-it patch -n --verbose",
    "release-it-minor": "release-it minor -n --verbose",
    "release-it-major": "release-it major -n --verbose",
    "release": "release-it"
  },
  "author": "philippone",
  "license": "ISC",
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "devDependencies": {
    "@angular/core": "8.2.13",
    "@babel/core": "7.7.2",
    "@storybook/addons": "5.2.6",
    "@storybook/cli": "5.2.6",
    "@storybook/html": "5.2.6",
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "babel-loader": "8.0.6",
    "cz-conventional-changelog": "3.0.2",
    "emotion": "10.0.23",
    "lit-html": "1.1.2",
    "release-it": "12.4.3",
    "typescript": "3.8.0-dev.20191105"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.11.0",
    "lit-html": "^1.1.2",
    "@storybook/addons": "^5.2.6",
    "@storybook/angular": "^5.2.6",
    "@storybook/api": "^5.2.6",
    "@storybook/core-events": "^5.2.6"
  },
  "publishConfig": {
    "registry": "https://nexus.ergosign-projects.com/repository/es-npm-external/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
