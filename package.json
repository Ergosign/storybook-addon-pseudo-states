{
  "name": "@ergosign/storybook-addon-pseudo-states",
  "version": "0.0.1",
  "description": "Storybook Addon to show pseudo states",
  "directories": {
    "test": "test",
    "dist": "dist"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch --outDir dist",
    "prepack-package": "cp package.json ./dist",
    "pack": "npm run build && npm run prepack-package && cd dist && npm pack",
    "release-it-beta": "release-it -n --preRelease=beta --no-npm.publish --verbose",
    "release-it-patch": "release-it patch -n --no-npm.publish --verbose ",
    "release-it-minor": "release-it minor -n --no-npm.publish --verbose",
    "release-it-major": "release-it major -n --no-npm.publish --verbose",
    "release": "release-it --no-npm.publish",
    "build-angular": "tsc --p src/angular"
  },
  "author": "philippone",
  "license": "MIT",
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "devDependencies": {
    "@angular/core": "8.2.14",
    "@babel/core": "7.7.2",
    "@storybook/addons": "5.2.6",
    "@storybook/cli": "5.2.6",
    "@storybook/html": "5.2.6",
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "babel-loader": "8.0.6",
    "cz-conventional-changelog": "3.0.2",
    "emotion": "10.0.23",
    "lit-html": "1.1.2",
    "release-it": "12.4.3",
    "rollup": "^1.27.0",
    "rollup-plugin-dts": "^1.1.12",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-api-guardian": "^0.4.6",
    "tslib": "^1.10.0",
    "typescript": "^3.8.0-dev.20191120"
  },
  "dependencies": {
    "@wessberg/rollup-plugin-ts": "^1.1.80"
  },
  "peerDependencies": {
    "react": "^16.11.0",
    "lit-html": "^1.1.2",
    "@storybook/addons": "^5.2.6",
    "@storybook/api": "^5.2.6",
    "@storybook/core-events": "^5.2.6",
    "@storybook/components": "^5.2.6"
  },
  "private": true,
  "publishConfig": {
    "registry": "https://nexus.ergosign-projects.com/repository/es-npm-external/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
